<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
  <constant name="struts.action.extension" value="action,,"/>

  <!-- Needed to run in production mode.  Must match name of compiled jar for this project. -->
  <constant name="struts.convention.action.includeJars" value=".*/frontend-1.0.jar.*" />
  <!-- Needed for Jetty in both production and development mode. -->
  <constant name="struts.convention.exclude.parentClassLoader" value="false" />
</struts>
